{
   "name": "auth-api-node-type-orm",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/express": "^4.17.21",
      "@types/http-errors": "^2.0.4",
      "@types/jest": "^29.5.12",
      "@types/jsonwebtoken": "^9.0.5",
      "@types/memory-cache": "^0.2.5",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.20.2",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.1",
      "concurrently": "^8.2.2",
      "dotenv": "^16.4.5",
      "express": "^4.18.2",
      "http-errors": "^2.0.0",
      "jest": "^29.7.0",
      "jsonwebtoken": "^9.0.2",
      "mariadb": "^3.2.3",
      "memory-cache": "^0.2.0",
      "mysql": "^2.14.1",
      "nodemon": "^3.1.0",
      "reflect-metadata": "^0.2.1",
      "typeorm": "0.3.20"
   },
   "scripts": {
      "watch": "tsc -w",
      "dev": "nodemon build/index.js",
      "start:dev": "concurrently \"tsc -w\" \"nodemon build/index.js\"",
